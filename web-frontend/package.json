{
  "name": "web-frontend",
  "private": true,
  "version": "0.4.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec 'NODE_ENV=development node --env-file='.env.local' --import=tsx server'",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/ServerApp.tsx",
    "preview": "nodemon --exec 'NODE_ENV=production node --env-file='.env.local' --import=tsx server'",
    "start": "NODE_ENV=production node --env-file='.env' --import=tsx server",
    "compile": "tsc",
    "clean-install": "rm -f -- package-lock.json && rm -rf node_modules && npm i",
    "eslint": "eslint .",
    "eslint-fix": "npm run eslint --fix",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src"
  },
  "dependencies": {
    "antd": "5.26.0",
    "axios": "1.9.0",
    "compression": "1.8.0",
    "d3": "7.9.0",
    "express": "5.1.0",
    "file-saver": "2.0.5",
    "openchemlib": "8.18.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "10.1.0",
    "react-mf": "3.1.0",
    "react-ocl": "7.0.2",
    "react-plotly.js": "2.6.0",
    "react-router-dom": "7.6.2",
    "remark-gfm": "4.0.1",
    "sirv": "3.0.1",
    "xml-formatter": "3.6.6"
  },
  "devDependencies": {
    "@ant-design/cssinjs": "1.23.0",
    "@babel/preset-react": "7.27.1",
    "@babel/preset-typescript": "7.27.1",
    "@types/compression": "1.8.1",
    "@types/d3": "7.4.3",
    "@types/express": "5.0.3",
    "@types/file-saver": "2.0.7",
    "@types/node": "24.0.0",
    "@types/react": "19.1.7",
    "@types/react-dom": "19.1.6",
    "@types/react-modal": "3.16.3",
    "@types/react-plotly.js": "2.6.3",
    "@typescript-eslint/eslint-plugin": "8.34.0",
    "@typescript-eslint/parser": "8.34.0",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "9.28.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.4.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.2.0",
    "nodemon": "3.1.10",
    "rimraf": "6.0.1",
    "sass": "1.89.2",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "typescript-eslint": "8.34.0",
    "vite": "6.3.5",
    "vitest": "3.2.3"
  }
}

{
  "name": "web-frontend",
  "private": true,
  "version": "0.4.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec 'NODE_ENV=development node --env-file='.env.local' --import=tsx server'",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/ServerApp.tsx",
    "preview": "nodemon --exec 'NODE_ENV=production node --env-file='.env.local' --import=tsx server'",
    "start": "NODE_ENV=production node --env-file='.env' --import=tsx server",
    "compile": "tsc",
    "clean-install": "rm -f -- package-lock.json && rm -rf node_modules && npm i",
    "eslint": "eslint .",
    "eslint-fix": "npm run eslint --fix",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src"
  },
  "dependencies": {
    "antd": "6.0.1",
    "axios": "1.13.2",
    "compression": "1.8.1",
    "d3": "7.9.0",
    "express": "5.2.1",
    "file-saver": "2.0.5",
    "openchemlib": "9.18.2",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-markdown": "10.1.0",
    "react-mf": "3.1.1",
    "react-ocl": "8.4.0",
    "react-plotly.js": "2.6.0",
    "react-router-dom": "7.10.0",
    "remark-gfm": "4.0.1",
    "sirv": "3.0.2",
    "xml-formatter": "3.6.7"
  },
  "devDependencies": {
    "@ant-design/cssinjs": "2.0.1",
    "@babel/preset-react": "7.28.5",
    "@babel/preset-typescript": "7.28.5",
    "@types/compression": "1.8.1",
    "@types/d3": "7.4.3",
    "@types/express": "5.0.6",
    "@types/file-saver": "2.0.7",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/react-modal": "3.16.3",
    "@types/react-plotly.js": "2.6.3",
    "@typescript-eslint/eslint-plugin": "8.48.1",
    "@typescript-eslint/parser": "8.48.1",
    "@vitejs/plugin-react": "5.1.1",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "globals": "16.5.0",
    "nodemon": "3.1.11",
    "patch-package": "8.0.1",
    "rimraf": "6.1.2",
    "sass": "1.94.2",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.1",
    "vite": "7.2.6",
    "vitest": "4.0.15"
  }
}

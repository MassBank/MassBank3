{
  "name": "web-frontend",
  "private": true,
  "version": "0.4.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec 'NODE_ENV=development node --env-file='.env.local' --import=tsx server'",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/ServerApp.tsx",
    "preview": "nodemon --exec 'NODE_ENV=production node --env-file='.env.local' --import=tsx server'",
    "start": "NODE_ENV=production node --env-file='.env' --import=tsx server",
    "compile": "tsc",
    "clean-install": "rm -f -- package-lock.json && rm -rf node_modules && npm i",
    "eslint": "eslint .",
    "eslint-fix": "npm run eslint --fix",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src"
  },
  "dependencies": {
    "antd": "5.23.3",
    "axios": "1.7.9",
    "compression": "1.7.5",
    "d3": "7.9.0",
    "express": "5.0.1",
    "file-saver": "2.0.5",
    "node-cache": "5.1.2",
    "openchemlib": "8.17.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-mf": "3.0.0",
    "react-ocl": "7.0.2",
    "react-router-dom": "7.1.3",
    "sirv": "3.0.0",
    "xml-formatter": "3.6.4"
  },
  "devDependencies": {
    "@ant-design/cssinjs": "1.23.0",
    "@ant-design/plots": "2.3.3",
    "@ant-design/v5-patch-for-react-19": "1.0.3",
    "@babel/preset-react": "7.26.3",
    "@babel/preset-typescript": "7.26.0",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@types/compression": "1.7.5",
    "@types/d3": "7.4.3",
    "@types/express": "5.0.0",
    "@types/file-saver": "2.0.7",
    "@types/node": "22.12.0",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@types/react-modal": "3.16.3",
    "@typescript-eslint/eslint-plugin": "8.22.0",
    "@typescript-eslint/parser": "8.22.0",
    "@vitejs/plugin-react": "4.3.4",
    "eslint": "9.19.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.18",
    "globals": "15.14.0",
    "nodemon": "3.1.9",
    "rimraf": "6.0.1",
    "sass": "1.83.4",
    "tsx": "4.19.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.22.0",
    "vite": "6.0.11"
  }
}
